{
    "manifest_version": 2,
    "name": "Sticky Notes Tab",
    "short_name": "Stick Notes Tab",
    "version": "2.0.1",
    "description": "Simple sticky notes board - tab extension",
    "author": "Mustafa Al-Saegh",
    "homepage_url": "http://mustafawatha.com",
    "permissions": ["storage"],
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "icons": {
        "16": "icons/16icon.png",
        "32": "icons/32icon.png",
        "48": "icons/48icon.png",
        "128": "icons/128icon.png"
    }
}
